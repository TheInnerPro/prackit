<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @vtype do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, :placeholder => "Type the vendor type name here", class: "form-control" %>

      <%= f.submit "Update Vendor Type", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
